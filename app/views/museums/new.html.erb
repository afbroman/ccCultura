<%= form_for museum  do |form| %>

  <%= form.error_messages %>

  <%= form.label :title  %> <br />
  <%= form.text_field :title %><br />

  <%= form.label :address %><br />
  <%= form.text_area :address %> <br />

  <%= form.label :description %><br />
  <%= form.text_area :description %><br />

  <%= form.submit %>

<% end %>